{
  "ProviderEnabled": false,
  "CacheEnabled": false,
  "ProviderUrl": "https://some-url.com",
  "Serilog": {
    "MinimumLevel": "Debug",
    "Override": {
      "Microsoft": "Warning"
    },
    "Using": [
      "Serilog.Sinks.File"
    ],
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "path": "Logs/log.txt"
        }
      }
    ]
  }
}
